<% @artist.editorial.each do |editorial| %>
    <div class="col-4 col-sm-2 editorial">
        <%= link_to image_tag((editorial.img_url.url || 'grey_square.svg'), class: "img-fluid w-100"), editorial.link_url %>
        <%= link_to "X", artist_editorial_path(editorial.artist, editorial), method: :delete, data: { confirm: 'Are you sure?'}, class: 'delete' %>
    </div>
<% end %>